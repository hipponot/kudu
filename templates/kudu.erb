---
:project:
 :name: <%= project_name %>
 :type: <%= project_type %>
:publications:
<% publications.each do |pub| %>
- :name: <%= pub[:name] %>
  :version: <%= pub[:version] %>
  :group: <%= pub[:group] %>
  :type: <%= pub[:type] %>
<% end %>
:dependencies: <%if dependencies.empty? %> [] <% end %>
<% dependencies.each do |dep| %>
- :name: <%= dep[:name] %>
	<% if dep[:version] %>
  :version: <%= dep[:version] %> 
  <% end %>
  :group: <%= dep[:group] %>
  :type: <%= dep[:type] %>
<% end %>
...
   